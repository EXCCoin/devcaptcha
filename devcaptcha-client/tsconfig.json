{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "jsx": "react",
    "target": "es6",
    "lib": [
      "es6",
      "dom",
      "webworker",
      "es2021"
    ],
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}